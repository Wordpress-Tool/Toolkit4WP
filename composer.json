{
    "name": "szepeviktor/toolkit4wp",
    "description": "OOP toolkit for daily tasks in WordPress development.",
    "keywords": [
        "tools",
        "oop",
        "wordpress"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "automattic/phpcs-neutron-ruleset": "^1.5.3",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.4.4",
        "phpstan/phpstan-shim": "^0.11.8",
        "szepeviktor/phpstan-wordpress": "^0.3.0"
    },
    "suggest": {
    },
    "config": {
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "Toolkit4WP\\": "src/"
        }
    },
    "scripts": {
        "style": "phpcs -p --standard=PSR12 src/",
        "analyze": "phpstan analyze"
    }
}
